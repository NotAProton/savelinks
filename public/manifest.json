{
  "name": "Save Links",
  "description": "Save and share ",
  "version": "1.1",
  "manifest_version": 3,
  "key": "",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_title": "Save Links",
    "default_popup": "index.html#popup"
  },
  "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
